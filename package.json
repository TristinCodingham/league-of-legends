{
  "name": "league-of-legends",
  "version": "1.0.0",
  "description": "A fictional gaming webapp for League of Legends (Riot Games)",
  "main": "index.js",
  "scripts": {
    "install": "npm install && cd client/ && npm install",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "cd client/ && npm start",
    "webapp-dev": "concurrently \"npm run server\" \"npm run client\" ",
    "webapp-prod": "concurrently \"npm start\" \"npm run client\" ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TristinCodingham/league-of-legends.git"
  },
  "keywords": [],
  "author": "Tristin Cunningham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TristinCodingham/league-of-legends/issues"
  },
  "homepage": "https://github.com/TristinCodingham/league-of-legends#readme",
  "dependencies": {
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "node-fetch": "^3.1.0"
  },
  "devDependencies": {
    "dot-env": "^0.0.1",
    "nodemon": "^2.0.15"
  }
}
